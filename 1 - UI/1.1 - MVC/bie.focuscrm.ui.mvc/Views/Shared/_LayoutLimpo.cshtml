

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="description" content="Sistema CRM">
    <meta name="author" content="">
    <title>Sistema CRM - @ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="~/Content/lib/stroke-7/style.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/lib/jquery.nanoscroller/css/nanoscroller.css" />
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <link type="text/css" href="~/Content/css/template.css" rel="stylesheet">
    @*<link rel="shortcut icon" href='@Url.Content("~/content/imagens/favicon.ico")' />*@
    

</head>
<body>
<div class="am-wrapper">

    <div class="am-content">
        <div class="main-content">

            <!-- inicio do conteúdo da página -->
            @RenderBody()
            <!-- fim do conteúdo da página -->

        </div>
    </div>

</div>

    @RenderSection("scripts_pre", required: false)

    <script src="~/Content/lib/jquery/jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/lib/jquery.nanoscroller/javascripts/jquery.nanoscroller.min.js" type="text/javascript"></script>
    <script src="~/Scripts/main.js" type="text/javascript"></script>
    <script src="~/Content/lib/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>    
    <script src="~/Content/lib/jquery-flot/jquery.flot.js" type="text/javascript"></script>
    <script src="~/Content/lib/jquery-flot/jquery.flot.pie.js" type="text/javascript"></script>
    <script src="~/Content/lib/jquery-flot/jquery.flot.resize.js" type="text/javascript"></script>
    <script src="~/Content/lib/jquery-flot/plugins/jquery.flot.orderBars.js" type="text/javascript"></script>
    <script src="~/Content/lib/jquery-flot/plugins/curvedLines.js" type="text/javascript"></script>
    <script src="~/Content/lib/jquery.sparkline/jquery.sparkline.min.js" type="text/javascript"></script>
    <script src="~/Content/lib/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>    
    <script src="~/Content/lib/countup/countUp.min.js" type="text/javascript"></script>
    <script src="~/Content/lib/chartjs/Chart.min.js" type="text/javascript"></script>

    @RenderSection("scripts", required: false)
    

</body>
</html>



